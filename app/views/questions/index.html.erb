<h1>Questions</h1>
<ul id="question-list">
<% @questions.each do |question| %>
  <li class="question">
    <h3><%= link_to markdown(question.title), question %></h3>
    <p>
      <%= link_to 'Edit', edit_question_path(question) %> |
      <%= link_to 'Destroy', question, method: :delete, data: {confirm: 'Are you sure?'} %>
    </p>
  </li>
<% end %>
</ul>
<%= link_to 'Post a new question', new_question_path %>
